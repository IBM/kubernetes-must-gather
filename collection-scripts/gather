SCRIPT_NAME="$(basename "${0}")"

source "$(dirname "${0}")/utilities/utilities.sh"
source "$(dirname "${0}")/utilities/cmdline.sh"

run() {
  startRun
  setDefaultFlags
  processCommandLine "${@}"
  defaultProcessing
  endRun
}

run "${@}" 2>&1 | tee -a "${TEE_FILE}"
